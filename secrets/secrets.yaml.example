# Example sops secrets file for provider deployments.
#
# Copy to secrets/secrets.yaml and encrypt with sops.
#
# keys:
#   - &default age1example...
# creation_rules:
#   - path_regex: .*
#     key_groups:
#       - age:
#           - *default
#
sample_project:
  backups_secret: ENC[AES256_GCM,data:example,iv:example,tag:example,type:str]
  backups_env: ENC[AES256_GCM,data:example,iv:example,tag:example,type:str]
  logging_env: ENC[AES256_GCM,data:example,iv:example,tag:example,type:str]
